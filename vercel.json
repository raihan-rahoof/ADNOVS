{
  "version": 2,
  "builds": [
    {
      "src": "crud/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
        "src":"build.sh",
        "use":"@vercel/static-build",
        "config":{
            "distDir":"staticfiles_build"
        }
    }
  ],
  "routes": [
    {
      "src": "/(.*)", "dest": "crud/wsgi.py"
    }
  ]
}